{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "iot:Publish",
        "iot:Receive"
      ],
      "Resource": [
        "arn:aws:iot:us-east-2:335064384801:topic/remember",
        "arn:aws:iot:us-east-2:335064384801:topic/unlock"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "iot:Subscribe"
      ],
      "Resource": [
        "arn:aws:iot:us-east-2:335064384801:topicfilter/remember",
        "arn:aws:iot:us-east-2:335064384801:topicfilter/unlock"
      ]
    }
  ]
}